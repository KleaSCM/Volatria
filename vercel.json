{
  "version": 2,
  "builds": [
    {
      "src": "volatriafront/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/volatriafront/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/volatriafront/$1"
    }
  ]
} 